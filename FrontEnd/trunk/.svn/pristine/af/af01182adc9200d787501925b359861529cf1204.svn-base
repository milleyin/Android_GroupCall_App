<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">
    <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:layout_marginRight="18dp"
                    android:gravity="left">
    <ImageView
        android:id="@+id/img_group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="18dp"
        android:layout_marginLeft="18dp"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/title"
        android:src="@drawable/icon_list_select_contact_income"
        android:visibility="visible"/>
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="fdasfaffdasfadffdasfafafafafafafafdsfasfadsdfasdf"
        android:textSize="16sp"
        android:textColor="@color/text_level_2"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/group_member_count"/>
    <TextView
        android:id="@+id/group_member_count"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:textSize="16sp"
        android:textColor="@color/text_level_2"
        android:text=""
        android:layout_alignParentRight="true"
        android:visibility="visible"/>
    </RelativeLayout>
</RelativeLayout>