package com.afmobi.palmchat.eventbusmodel;

/**
 * Created by Transsion on 2016/9/6.
 */
public class EventPalmcallServiceNotication {
    /**显示状态*/
    private String stateString;
    private boolean isRunning;

    public EventPalmcallServiceNotication(String stateString) {
        this.stateString = stateString;
    }

    public EventPalmcallServiceNotication(String stateString, boolean isRunning) {
        this.stateString = stateString;
        this.isRunning = isRunning;
    }

    public String getStateString() {
        return stateString;
    }

    public boolean isRunning() {
        return isRunning;
    }

    public void setRunning(boolean running) {
        isRunning = running;
    }

    public void setStateString(String stateString) {

        this.stateString = stateString;
    }
}
