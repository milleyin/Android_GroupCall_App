package com.afmobi.palmchat.eventbusmodel;

public class ShowPublicAccountsEvent {
	private boolean isShowPublic;
	private String publicAccountName;
	
	public ShowPublicAccountsEvent(boolean isShowPublic,String accountName){
		this.isShowPublic = isShowPublic;
		this.publicAccountName = accountName;
	}
	public boolean isShowPublic() {
		return isShowPublic;
	}
	public void setShowPublic(boolean isShowPublic) {
		this.isShowPublic = isShowPublic;
	}
	public String getPublicAccountName() {
		return publicAccountName;
	}
	public void setPublicAccountName(String publicAccountName) {
		this.publicAccountName = publicAccountName;
	}
}
