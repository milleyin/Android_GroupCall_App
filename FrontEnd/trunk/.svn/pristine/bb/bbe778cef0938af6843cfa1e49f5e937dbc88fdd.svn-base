<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/r_paofile"
    style="@style/SystemBarTint_Style"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_login_bg"
    android:orientation="vertical">

    <RelativeLayout
        style="@style/BaseBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@id/title_layout"
            layout="@layout/head_friend_title_more" />

        <ScrollView
            android:id="@id/friend_profile"
            style="@style/BaseBackground"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/title_layout"
            android:fadingEdge="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@id/profile_status_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone">

                    <TextView
                        android:id="@id/profile_status_sort"
                        android:layout_width="fill_parent"
                        android:layout_height="20dp" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/profile_ba_layout1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="3dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_marginTop="8dp"
                        android:visibility="gone" />

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/broadcast_trending_cccccc" />

                    <RelativeLayout
                        android:id="@+id/profile_photo_layout"
                        style="@style/profile_layout1">

                        <TextView
                            android:id="@+id/profile_photo_key"
                            style="@style/profile_detail_key"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="18dip"
                            android:text="@string/set_picture" />

                        <ImageView
                            android:id="@+id/arrow_photo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/profile_photo_value"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="12dp"
                            android:layout_toLeftOf="@id/arrow_photo" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_photo_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/profile_whatsup_layout"
                        style="@style/profile_layout1">

                        <ImageView
                            android:id="@+id/img_arrow"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@+id/profile_whatsup_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="18dip"
                            android:text="@string/whatsup" />

                        <TextView
                            android:id="@+id/profile_whatsup_value"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@+id/img_arrow"
                            android:layout_toRightOf="@+id/profile_whatsup_key"
                            android:gravity="right"
                            android:paddingRight="12dp"
                            android:textColor="@color/color_btn" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_whatsup_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>

                    <!--<RelativeLayout
                        android:id="@+id/profile_background_layout"
                        style="@style/profile_layout1">

                        <ImageView
                            android:id="@+id/arrow_region"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@+id/profile_background_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="18dip"
                            android:text="@string/background" />

                        <TextView
                            android:id="@+id/profile_background_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@+id/arrow_region"
                            android:layout_toRightOf="@+id/profile_background_key" />
                    </RelativeLayout>-->

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.1dp"
                        android:background="@color/broadcast_trending_cccccc" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/profile_ba_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dip"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/profile_experience_sort_1"
                        android:layout_width="fill_parent"
                        android:layout_height="3dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_marginTop="8dp"
                        android:visibility="gone" />

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.1dip"
                        android:background="@color/broadcast_trending_cccccc" />

                    <RelativeLayout
                        android:id="@id/profile_name_layout"
                        style="@style/profile_layout1">

                        <TextView
                            android:id="@id/profile_name_key"
                            style="@style/profile_detail_key"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="18dip"
                            android:text="@string/name" />

                        <ImageView
                            android:id="@+id/arrow_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@id/profile_name_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@id/arrow_name"
                            android:layout_toRightOf="@id/profile_name_key"
                            android:background="@null"
                            android:focusableInTouchMode="true"
                            android:inputType="none"
                            android:singleLine="true" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_name_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@id/profile_brith_layout"
                        style="@style/profile_layout1">

                        <ImageView
                            android:id="@+id/arrow_birthday"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@id/profile_brith_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="18dip"
                            android:text="@string/dob" />

                        <TextView
                            android:id="@id/profile_brith_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@id/arrow_birthday"
                            android:layout_toRightOf="@id/profile_brith_key" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_brith_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@id/profile_sex_layout"
                        style="@style/profile_layout1">

                        <ImageView
                            android:id="@id/arrow_region"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="20dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@id/profile_sex_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="18dip"
                            android:text="@string/gender" />

                        <TextView
                            android:id="@id/profile_sex_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@id/arrow_region"
                            android:layout_toRightOf="@id/profile_sex_key" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_sex_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@id/profile_region_layout"
                        style="@style/profile_layout1">

                        <ImageView
                            android:id="@id/arrow_region"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="18dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@id/profile_region_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="18dip"
                            android:text="@string/region" />

                        <TextView
                            android:id="@id/profile_region_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@id/arrow_region"
                            android:layout_toRightOf="@id/profile_region_key"
                            android:singleLine="true" />

                        <View
                            android:layout_width="fill_parent"
                            android:layout_height="0.1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@+id/profile_region_key"
                            android:background="@color/predict_e6" />
                    </RelativeLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.1dip"
                        android:background="@color/broadcast_trending_cccccc" />
                </LinearLayout>

                <LinearLayout
                    android:id="@id/profile_experience_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dip"
                    android:background="@color/white"
                    android:visibility="gone"
                    android:orientation="vertical">

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.1dip"
                        android:background="@color/broadcast_trending_cccccc" />

                    <RelativeLayout
                        android:id="@id/profile_status_sign_layout"
                        style="@style/profile_layout1"
                        android:visibility="gone">

                        <ImageView
                            android:id="@id/arrow_region"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dip"
                            android:src="@drawable/navigation_forward"
                            android:visibility="visible" />

                        <TextView
                            android:id="@id/profile_status_key"
                            style="@style/profile_detail_key"
                            android:layout_marginLeft="10dip"
                            android:text="@string/whatsup" />

                        <TextView
                            android:id="@id/profile_status_value"
                            style="@style/profile_detail_value"
                            android:layout_toLeftOf="@id/arrow_region"
                            android:layout_toRightOf="@id/profile_status_key"
                            android:ellipsize="marquee"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:scrollbars="none"
                            android:singleLine="true"
                            android:text="@string/default_status" />
                    </RelativeLayout>



                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="0.1dip"
                        android:background="@color/broadcast_trending_cccccc" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>

</RelativeLayout>