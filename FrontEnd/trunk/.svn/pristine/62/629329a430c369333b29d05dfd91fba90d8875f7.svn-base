package com.afmobi.palmchat.ui.activity.main.model;


/**
 * @author Stone
 *
 */
public class DialogItem {
	private  boolean mIsSpecial;
	private int mTextId;
	private int mViewId;
	private int mImageId;
	private int mType;
	private int mPosition;
	private Object mObject;
	public  DialogItem(int textId, int viewId){
		mTextId = textId;
		mViewId = viewId;
	}
	
	/*public  DialogItem(int textId, int viewId,int type){
		mTextId = textId;
		mViewId = viewId;
		mType = type;
	}*/

	public DialogItem(int textId, int viewId, int imageId){
		mTextId = textId;
		mViewId = viewId;
		mImageId = imageId;
	}

	public  DialogItem(int textId, int viewId,int position,int imageId) {
		mTextId = textId;
		mViewId = viewId;
		mPosition = position;
		mImageId = imageId;
	}

	public  DialogItem(int textId, int viewId,int position,int imageId,int type) {
		mTextId = textId;
		mViewId = viewId;
		mPosition = position;
		mImageId = imageId;
		mType = type;
	}


	public  DialogItem(int textId, int viewId,int position,int type,int imageId,Object obj) {
		mTextId = textId;
		mViewId = viewId;
		mPosition = position;
		mType = type;
		mImageId = imageId;
		mObject = obj;
	}

	public boolean isSpecial() {
		return mIsSpecial;
	}

	public void setSpecial(boolean mIsSpecial) {
		this.mIsSpecial = mIsSpecial;
	}

	public int getTextId() {
		return mTextId;
	}

	public void setTextId(int mTextId) {
		this.mTextId = mTextId;
	}

	public int getViewId() {
		return mViewId;
	}

	public void setViewId(int mViewId) {
		this.mViewId = mViewId;
	}
	
	public int getmType() {
		return mType;
	}

	public int getImageId() {
		return mImageId;
	}

	public int getPositionId() {
		return mPosition;
	}

	public Object getObject() {
		return mObject;
	}

	public void setmType(int mType) {
		this.mType = mType;
	}

}

