package com.afmobi.palmchat.ui.activity.palmcall.model;

import java.io.Serializable;

/**
 * 语音拨打电话的过程中需要传递的json数据
 * Created by Transsion on 2016/8/8.
 */
public class JustalkDataInfo  implements Serializable{

    public static final String AFAFIDKEY = "AFAfIdKey";
    public static final String AFNAMEKEY = "AFNameKey";
    public static final String AFSEXKEY = "AFSexKey";
    public static final String AFAGEKEY = "AFAgeKey";
    public static final String AFCOVERURLKEY = "AFCoverUrlKey";
    public static final String AFMEDIAURLKEY = "AFMediaUrlKey";
    /**对方id*/
    private String AFAfIdKey;
    /**对方名字*/
    private String AFNameKey;
    /**对方性别*/
    private int AFSexKey;
    /**对方年龄*/
    private int AFAgeKey;
    /**对方头像地址*/
    private String AFCoverUrlKey;
    /***主对方语音地址*/
    private String AFMediaUrlKey;

    public JustalkDataInfo() {
        this.AFAfIdKey = "";
        this.AFNameKey = "";
        this.AFSexKey = 0;
        this.AFAgeKey = 0;
        this.AFCoverUrlKey = "";
        this.AFMediaUrlKey = "";
    }

    public JustalkDataInfo(String AFAfIdKey, String AFNameKey, int AFSexKey, int AFAgeKey, String AFCoverUrlKey, String AFMediaUrlKey) {
        this.AFAfIdKey = AFAfIdKey;
        this.AFNameKey = AFNameKey;
        this.AFSexKey = AFSexKey;
        this.AFAgeKey = AFAgeKey;
        this.AFCoverUrlKey = AFCoverUrlKey;
        this.AFMediaUrlKey = AFMediaUrlKey;
    }

    public String getAFAfIdKey() {
        return AFAfIdKey;
    }

    public String getAFNameKey() {
        return AFNameKey;
    }

    public int getAFSexKey() {
        return AFSexKey;
    }

    public int getAFAgeKey() {
        return AFAgeKey;
    }

    public String getAFCoverUrlKey() {
        return AFCoverUrlKey;
    }

    public String getAFMediaUrlKey() {
        return AFMediaUrlKey;
    }

    public void setAFAfIdKey(String AFAfIdKey) {
        this.AFAfIdKey = AFAfIdKey;
    }

    public void setAFNameKey(String AFNameKey) {
        this.AFNameKey = AFNameKey;
    }

    public void setAFSexKey(int AFSexKey) {
        this.AFSexKey = AFSexKey;
    }

    public void setAFAgeKey(int AFAgeKey) {
        this.AFAgeKey = AFAgeKey;
    }

    public void setAFCoverUrlKey(String AFCoverUrlKey) {
        this.AFCoverUrlKey = AFCoverUrlKey;
    }

    public void setAFMediaUrlKey(String AFMediaUrlKey) {
        this.AFMediaUrlKey = AFMediaUrlKey;
    }


    @Override
    public String toString() {
        return "JustalkDataInfo{" +
                "AFAfIdKey='" + AFAfIdKey + '\'' +
                ", AFNameKey='" + AFNameKey + '\'' +
                ", AFSexKey=" + AFSexKey +
                ", AFAgeKey=" + AFAgeKey +
                ", AFCoverUrlKey='" + AFCoverUrlKey + '\'' +
                ", AFMediaUrlKey='" + AFMediaUrlKey + '\'' +
                '}';
    }
}
